{
  "name": "ory-admin-ui",
  "version": "1.0.0",
  "description": "Production-grade admin UI for Ory Kratos identity server",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "oxlint src/",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.62.8",
    "@vueuse/core": "^12.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ky": "^1.7.4",
    "loglevel": "^1.9.2",
    "lucide-vue-next": "^0.468.0",
    "pinia": "^2.3.0",
    "radix-vue": "^1.9.11",
    "tailwind-merge": "^2.6.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-sonner": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "jsdom": "^25.0.1",
    "oxlint": "^0.15.3",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meysam81/ory-admin-ui"
  },
  "keywords": [
    "ory",
    "kratos",
    "admin",
    "identity",
    "vue",
    "typescript"
  ],
  "author": "meysam81",
  "license": "Apache-2.0"
}
